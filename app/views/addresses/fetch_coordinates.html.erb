<h1>Coordinates Lookup</h1>
<h2>Insert address to lookup</h2>

<form>
<div>
    <label for="the_address">Address:</label>
    <input id="the_address" type="text" name="address">
 </div>
 <div>
    <input type="submit" value="Get Coordinates">
  </div>
</form>

<dl>
  <dt>You inputted the following address:</dt>
  <dd><%= @address %></dd>

  
  <dt>Latitude:</dt>
  <dd><%= @latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @longitude %></dd>
</dl>


